{
  "expo": {
    "name": "VBR Remote",
    "slug": "vbr-remote",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366F1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kaito1023.vbr"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6366F1"
      },
      "package": "com.kaito1023.vbr",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "react-native-fast-tflite",
      "expo-asset",
      [
        "expo-image-picker",
        {
          "photosPermission": "VBRアプリが写真ライブラリにアクセスすることを許可します。"
        }
      ]
    ],
    "assetBundlePatterns": [
      "assets/**/*"
    ]
  }
}
